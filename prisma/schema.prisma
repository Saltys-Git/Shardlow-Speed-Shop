generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Images {
  id            Int       @id @default(autoincrement())
  url           String[]  
  serviceId     String?
  showInGallery Boolean
  uploadedAt    DateTime  @updatedAt
}

model Users {
  id            Int             @id @default(autoincrement())
  email         String
  loginSessions LoginSessions[]
  tempOTP       String
}

model LoginSessions {
  id       Int                   @id @default(autoincrement())
  loggedAt DateTime @updatedAt
  ip       String
  Users    Users    @relation(fields: [id], references: [id])
}
