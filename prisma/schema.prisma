generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TableName {
  id        Int      @id @default(autoincrement())
  url       String
  serverid  String?
  updatedAt DateTime @updatedAt
}

model Users {
  id            Int             @id @default(autoincrement())
  email         String
  loginSessions LoginSessions[]
}

model LoginSessions {
  id       Int      @id @default(autoincrement())
  loggedAt DateTime
  ip       String
  userId   Int
  Users    Users    @relation(fields: [userId], references: [id])
}
