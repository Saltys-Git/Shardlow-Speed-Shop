generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Images {
  id        Int      @id @default(autoincrement())
  url       String
  serviceId  String?
  updatedAt DateTime @updatedAt
}

model Users {
  id            Int             @id @default(autoincrement())
  email         String
  loginSessions LoginSessions[]
}

model LoginSessions {
  id       Int      @id @default(autoincrement())
  loggedAt DateTime
  ip       String
  Users    Users    @relation(fields: [id], references: [id])
}
